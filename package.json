  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielhws/node-typescript-restify-api/issues"
  },
  "homepage": "https://github.com/gabrielhws/node-typescript-restify-api#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "log4js": "^0.3.3",
    "mongoose": "^5.2.0",
    "restify": "^7.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.110",
    "@types/log4js": "0.0.33",
