    "@types/restify-errors": "4.3.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "husky": "^0.14.3",
    "prettier": "^1.13.1",
    "pretty-quick": "^1.6.0",
    "restify-errors": "6.1.1"
